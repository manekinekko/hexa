{
  "name": "@manekinekko/hexa",
  "version": "1.0.0-preview.6",
  "description": "Hexa: The ultimate companion for the Azure CLI. Setup and deploy in seconds.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": {
    "hexa": "./build/index.js"
  },
  "homepage": "https://github.com/manekinekko/hexa#readme",
  "private": false,
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "rm -fr build/ && mkdir build && npm run copy && tsc",
    "copy": "cp -r src/templates build",
    "prepare": "npm run build",
    "release": "release-it --no-git.requireUpstream"
  },
  "keywords": [
    "azure",
    "cloud",
    "serverless",
    "hosting",
    "database",
    "authentication",
    "storage"
  ],
  "author": {
    "name": "Wassim Chegham",
    "email": "github@wassim.dev"
  },
  "bugs": {
    "url": "https://github.com/manekinekko/hexa/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manekinekko/hexa.git"
  },
  "license": "MIT",
  "dependencies": {
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^3.0.1",
    "configstore": "^5.0.0",
    "debug": "^4.1.1",
    "deepmerge": "^4.0.0",
    "dotenv": "^8.1.0",
    "ora": "^3.4.0",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.3",
    "inquirer": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@release-it/conventional-changelog": "^1.1.0",
    "@types/clear": "^0.1.0",
    "@types/configstore": "^4.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.3",
    "@types/ora": "^3.2.0",
    "husky": "^3.0.7",
    "nodemon": "^1.19.1",
    "release-it": "^12.4.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
